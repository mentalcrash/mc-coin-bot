{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "templating": {
    "list": [
      {
        "current": { "text": "All", "value": "$__all" },
        "definition": "label_values(mcbot_gbm_severity, strategy)",
        "name": "strategy",
        "type": "query",
        "includeAll": true,
        "multi": true,
        "datasource": { "type": "prometheus", "uid": "prometheus" }
      }
    ]
  },
  "panels": [
    {
      "title": "Row 1: Distribution Drift",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "KS Statistic",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "targets": [
        { "expr": "mcbot_distribution_ks_statistic{strategy=~\"$strategy\"}", "legendFormat": "{{strategy}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.2 },
              { "color": "red", "value": 0.4 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "P-Value",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "targets": [
        { "expr": "mcbot_distribution_p_value{strategy=~\"$strategy\"}", "legendFormat": "{{strategy}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "green", "value": 0.05 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Row 2: RANSAC Decay",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "collapsed": false
    },
    {
      "title": "RANSAC Slope",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 10 },
      "targets": [
        { "expr": "mcbot_ransac_slope{strategy=~\"$strategy\"}", "legendFormat": "{{strategy}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0 },
              { "color": "green", "value": 0.001 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Decay Detected",
      "type": "state-timeline",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 10 },
      "targets": [
        { "expr": "mcbot_ransac_decay_detected{strategy=~\"$strategy\"}", "legendFormat": "{{strategy}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": {
              "0": { "text": "OK", "color": "green" },
              "1": { "text": "DECAY", "color": "red" }
            }}
          ]
        },
        "overrides": []
      }
    },
    {
      "title": "Cumulative Return vs Conformal Lower",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 10 },
      "targets": [
        { "expr": "mcbot_ransac_current_cumulative{strategy=~\"$strategy\"}", "legendFormat": "Cumulative {{strategy}}", "refId": "A" },
        { "expr": "mcbot_ransac_conformal_lower{strategy=~\"$strategy\"}", "legendFormat": "Lower Bound {{strategy}}", "refId": "B" }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] }
    },
    {
      "title": "Row 3: GBM Drawdown",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
