        { "expr": "mcbot_gbm_drawdown_depth{strategy=~\"$strategy\"}", "legendFormat": "{{strategy}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.2 }
            ]
          }
        },
        "overrides": []
      }
    },
