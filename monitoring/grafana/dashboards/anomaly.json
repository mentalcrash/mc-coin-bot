              { "color": "red", "value": 30 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "GBM Severity",
      "type": "state-timeline",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 19 },
      "targets": [
        { "expr": "mcbot_gbm_severity{strategy=~\"$strategy\"}", "legendFormat": "{{strategy}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": {
